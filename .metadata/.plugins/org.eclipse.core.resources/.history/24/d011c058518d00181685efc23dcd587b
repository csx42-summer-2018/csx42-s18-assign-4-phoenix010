	@Override
	public int visitHeap(MyArray myArr) {
		
			this.size= myArr.getMyArr().size();
			this.heap = Arrays.copyOf(heap, size);
			for(int i=0; i< size; i++) {
				 heap[i] = myArr.getMyArr().get(i).intValue(); 
